#include <stdio.h>
#include <gmp.h>

#define M 1000

int main(void)
{
  mpz_t p, r;
  unsigned maxlen = 0, imax = 0;
  unsigned i;

  mpz_init(p); mpz_init(r);

  for (i = 2; i < M; i++) {
    unsigned len = 1;

    if (i % 2 == 0 || i % 5 == 0) {
      continue;
    }

    mpz_set_ui(p, 10);
    while (mpz_mod_ui(r, p, i), mpz_cmp_ui(r, 1) != 0) {
      gmp_printf("%Zd %Zd %d \n", r, p ,i);
      getchar();
      len++;
      mpz_mul_ui(p, p, 10);
    }
    gmp_printf("DONE : %Zd %Zd %d \n", r, p ,i);
      if (len > maxlen) {
      maxlen = len;
      imax = i;
    }

  }
  
  printf("%u\n", imax);

  mpz_clear(p);
  mpz_clear(r);

  return 0;
}
